{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=MTM_Db;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "JWT": {
    "SecretKey": "your-66-character-key" //create your own
  },
  "Geocoding": {
    "Key": "your-locationiq-key" //get your own at https://my.locationiq.com/dashboard/login?ref=locationiq
  },
  "Google":  {
    "ClientId": "your-client-id",
    "ClientSecret": "your-client-secret"
  },
  "Names": {
    "BaseFrontendURL": "http://localhost:5173"
  },
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com", //default for gmail
    "SmtpPort": 587, //default for gmail
    "SenderEmail": "your-email",
    "SenderName": "MTM Project",
    "SenderPassword": "account-or-application-password", //can be also Google application password - https://myaccount.google.com/u/1/apppasswords
    "EnableSsl": true
  }
}
